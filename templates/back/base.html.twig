<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}back{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('shared_styles') }}
            {{ encore_entry_link_tags('back_styles') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('shared_scripts') }}
            {{ encore_entry_script_tags('back_scripts') }}
        {% endblock %}
    </head>
    <body>
        <div class="row">
            <div class="col-3 leftBack">
                {% include 'back/partial/nav.html.twig' %}
            </div>
            <div class="col-9">
                {% block body %}
                {% endblock %}
            </div>
        </div>
    </body>
</html>
